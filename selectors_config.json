{
  "title": {
    "selector": "h1.title, .article-title, title",
    "attribute": null
  },
  "description": {
    "selector": "meta[name='description']",
    "attribute": "content"
  },
  "links": {
    "selector": "a",
    "attribute": "href",
    "list": true
  },
  "images": {
    "selector": "img",
    "attribute": "src",
    "list": true
  },
  "headings": {
    "selector": "h2, h3",
    "attribute": null,
    "list": true
  },
  "paragraphs": {
    "selector": "p",
    "attribute": null,
    "list": true
  }
}